		"@angular/compiler": "^12",
		"@angular/compiler-cli": "^12",
		"@angular/core": "^12",
		"@angular/forms": "^12",
		"@angular/material": "^12",
		"@angular/platform-browser": "^12",
		"@angular/platform-browser-dynamic": "^12",
		"@angular/router": "^12",
		"@cyph/prettier": "*",
		"@cyph/pretty-quick": "*",
		"@types/jasmine": "~3.10.0",
		"@types/node": "^16",
		"@types/webpack": "^5",
		"husky": "^7",
		"jasmine-core": "~3.10.0",
		"karma": "~6.3.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage": "~2.0.3",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "~1.7.0",
		"ng-packagr": "^12",
		"rxjs": "^6",
